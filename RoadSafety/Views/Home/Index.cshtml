@{
    ViewData["Title"] = "Home Page";
}
<div class="responsive">
<div class="home__banner">
	<div class="banner__slide swiper banner-slide swiper-fade swiper-initialized swiper-horizontal swiper-pointer-events">
		<div class="swiper-wrapper" id="swiper-wrapper-992bfe88fb0a49f6" aria-live="off" style="transition-duration: 0ms;">
			<div class="banner__slide-area swiper-slide swiper-slide-duplicate swiper-slide-duplicate-active" data-swiper-autoplay="6000" data-swiper-slide-index="1" role="group" aria-label="2 / 2" style="width: 1090px; opacity: 1; transform: translate3d(0px, 0px, 0px); transition-duration: 0ms;">
				<div class="banner__slide-area-image" data-background="assets/img/bg/bg-new.png" style="background-image: url(&quot;assets/img/bg/bg-new.png&quot;);"></div>
				<div class="container">
					<div class="row align-items-center">
						<div class="col-xl-8 order-last order-lg-first">
							<div class="banner__slide-content">
								<span class="subtitle__one" data-animation="fadeInLeft" data-delay=".4s" style="animation-delay: 0.4s;">Welcome to Our Barbex</span>
								<h1 data-animation="fadeInLeft" data-delay=".6s" class="" style="animation-delay: 0.6s;">Best Haircut Salon in the City</h1>
								<div class="banner__slide-content-button" data-animation="fadeInLeft" data-delay=".9s" style="animation-delay: 0.9s;">
									
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="banner__slide-right" data-animation="fadeInRight" data-delay="1.3s" style="animation-delay: 1.3s;">
					<img src="assets/img/bg/banner-2.png" alt="">
				</div>
			</div>
			<div class="banner__slide-area swiper-slide swiper-slide-prev swiper-slide-duplicate-next" data-swiper-autoplay="6000" data-swiper-slide-index="0" role="group" aria-label="1 / 2" style="width: 1090px; opacity: 1; transform: translate3d(-1090px, 0px, 0px); transition-duration: 0ms;">
				<div class="banner__slide-area-image" data-background="assets/img/bg/bg-new.png" style="background-image: url(&quot;assets/img/bg/bg-new.png&quot;);"></div>
				<div class="container">	
					<div class="row align-items-center">
						<div class="col-xl-8 order-last order-lg-first">
							<div class="banner__slide-content">
								
				
								<div class="banner__slide-content-button" data-animation="fadeInLeft" data-delay=".9s" style="animation-delay: 0.9s;">
								
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="banner__slide-right" data-animation="fadeInRight" data-delay="1.3s" style="animation-delay: 1.3s;">
					
				</div>
			</div>
			<div class="banner__slide-area swiper-slide swiper-slide-visible swiper-slide-active" data-swiper-autoplay="6000" data-swiper-slide-index="1" role="group" aria-label="2 / 2" style="width: 1090px; opacity: 1; transform: translate3d(-2180px, 0px, 0px); transition-duration: 0ms;">
				<div class="banner__slide-area-image" data-background="assets/img/bg/bg-new.png" style="background-image: url(&quot;assets/img/bg/bg-new.png&quot;);"></div>
				<div class="container">
					<div class="row align-items-center">
						<div class="col-xl-8 order-last order-lg-first">
							
						</div>
					</div>
				</div>
				<div class="banner__slide-right" data-animation="fadeInRight" data-delay="1.3s" style="animation-delay: 1.3s;">
					<img src="assets/img/bg/banner-2.png" alt="">
				</div>
			</div>
			<div class="banner__slide-area swiper-slide swiper-slide-duplicate swiper-slide-next swiper-slide-duplicate-prev" data-swiper-autoplay="6000" data-swiper-slide-index="0" role="group" aria-label="1 / 2" style="width: 1090px; opacity: 0; transform: translate3d(-3270px, 0px, 0px); transition-duration: 0ms;">
				<div class="banner__slide-area-image" data-background="assets/img/bg/bg-new.png" style="background-image: url(&quot;assets/img/bg/bg-new.png&quot;);"></div>
				<div class="container">
					<div class="row align-items-center">
						<div class="col-xl-8 order-last order-lg-first">
							
						</div>
					</div>
				</div>
				
			</div>
		</div>
		<div class="home__banner-button">
			<div class="home__banner-button-prev swiper-button-prev" tabindex="0" role="button" aria-label="Previous slide" aria-controls="swiper-wrapper-992bfe88fb0a49f6"></div>
			<div class="home__banner-button-next swiper-button-next" tabindex="0" role="button" aria-label="Next slide" aria-controls="swiper-wrapper-992bfe88fb0a49f6"></div>
		</div>
		<span class="swiper-notification" aria-live="assertive" aria-atomic="true"></span>
	</div>
</div>
